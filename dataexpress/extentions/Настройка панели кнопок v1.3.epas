function BnCapToTBN(Caption:string):integer;
begin

case trim(Caption) of
  'В начало': result := TBN_MOVEFIRST;
  'Предыдущая': result := TBN_MOVEPRIOR;
  'Следующая': result := TBN_MOVENEXT;
  'В конец': result := TBN_MOVELAST;
  'Добавить': result := TBN_APPEND;
  'Редактировать': result := TBN_EDIT;
  'Отмена': result := TBN_CANCEL;
  'Сохранить': result := TBN_POST;
  'Удалить': result := TBN_DELETE;
  'Обновить': result := TBN_REFRESH;
  'Печать': result := TBN_PRINT;
  'Фильтр': result := TBN_FILTER;
  'Поиск': result := TBN_SEARCH;
  'Справка': result := TBN_HELP;
end;
end;

function TBNByImageIndex(ImageIndex:integer):integer;
begin
case ImageIndex of
  5: result := TBN_MOVEFIRST;
  8: result := TBN_MOVEPRIOR;
  7: result := TBN_MOVENEXT;
  6: result := TBN_MOVELAST;
  1: result := TBN_APPEND;
  0: result := TBN_EDIT;
  16: result := TBN_CANCEL;
  15: result := TBN_POST;
  2: result := TBN_DELETE;
  10: result := TBN_REFRESH;
  9: result := TBN_PRINT;
  3: result := TBN_FILTER;
  12: result := TBN_SEARCH;
  18: result := TBN_HELP;
end;
end;

{@action
Id=4BC5175C-FA7C-406B-A325-EE980EF71EC8
Target=main
OrigName=ExToolbar
Name=Настройка панели кнопок в главном окне.
Group=Кастомизация интерфейса
UI=
<ui>
 <divider caption="(В скобках - значения по-умолчанию)" />
 <list name="ImgListSize" caption="Размер иконок в пикселях (24):"
 items="12;16;24;32" defaultvalue="24"/>
 <number name="IcoMargins" caption="Отступ от иконки до края кнопки (4):"
  defaultvalue="4"/>
 <checkbox name="NavBnsDisabled" caption="Отключить кнопки навигации" defaultvalue=1 />
 <checkbox name="ToolbarMoveDown" caption="Сместить панель кнопок ниже закладок"
 defaultvalue=1 />
<list name="ToolbarColor" caption="Цвет панели кнопок:"
items="Системный (по-умолчанию);Белый;Серебряный;Желтый;Кремовый"
defaultvalue="Системный (по-умолчанию)"/>
 <checkbox name="ToolbarBorder" caption="Рисовать рамку вокруг панели"
 defaultvalue=1 />
 <folder name="IconsFromFiles" caption="Собственные иконки (путь к папке):" />
 <checkbox name="FilterBnIndicator" caption="Включить индикатор установленного фильтра"
 defaultvalue=1 />


</ui>
Description=Действие позволяет выполнить кастомные настройки панели кнопок
главного окна программы.
@}

var OldMainWindow_pages_OnChange: TNotifyEvent;
    TB_MoveDown:boolean;
    NavBnsVisible:boolean;
    Old_MainWindowOnDataBaseClose:TNotifyEvent;

function GetActiveForm:TDxForm;
begin
  result:=MainWindow.FormViews[MainWindow.Pages.ActivePageIndex].Form;
end;

function DetectEnabledTbnButtons:boolean;
var i:integer;
begin
  result:=false;
    for i:=0 to MainWindow.Toolbar.ButtonCount-1 do
     if MainWindow.Toolbar.Buttons[i].Enabled then
      begin
        result:=true;
        break;
      end;
end;


procedure SetNavBnsVisible(b:boolean);
begin
with MainWindow.Toolbar do
  begin
    Buttons[tbn_movelast].Visible := b;
    if b then Buttons[tbn_movelast].Left:=0;
    Buttons[tbn_movenext].Visible := b;
    if b then Buttons[tbn_movenext].Left:=0;
    Buttons[tbn_moveprior].Visible := b;
    if b then Buttons[tbn_moveprior].Left:=0;
    Buttons[tbn_movefirst].Visible := b;
    if b then Buttons[tbn_movefirst].Left:=0;
  end;
end;

procedure MainWindow_pages_OnChange(sender: Tobject);
begin
    if OldMainWindow_pages_OnChange<>nil then
    OldMainWindow_pages_OnChange(sender);
    if TB_MoveDown then
    begin
        if GetActiveForm.ViewType = vtWithoutGrid then
        MainWindow.Toolbar.Parent := GetActiveForm.Parent
        else
        MainWindow.Toolbar.Parent := GetActiveForm.Grid.Parent;
		if  (GetActiveForm.ViewType = vtSimpleForm) and DetectEnabledTbnButtons
    then
        MainWindow.Toolbar.Parent := GetActiveForm.Parent;
    end;
    if not NavBnsVisible then
    if GetActiveForm.ViewType = vtWithoutGrid then
     SetNavBnsVisible(true)
        else
     SetNavBnsVisible(NavBnsVisible);
end;



////////////////////////////////////////////////////////////////////////////////
////////////////////////////// ИНДИКАТОР ФИЛЬТРА ///////////////////////////////
////////////////////////////////////////////////////////////////////////////////

var FT:ttimer;

procedure IndicateTbnFilter(Sender: TObject);
begin
// if MainWindow.FindComponent('DesignFr')<>nil then exit;
 if (MainWindow.StatusBar.Panels[3].Text='1') and
 not (MainWindow.Toolbar.Buttons[tbn_filter].Down) then
 MainWindow.Toolbar.Buttons[tbn_filter].Down:= true
 else
  if (MainWindow.StatusBar.Panels[3].Text<>'1') and
 (MainWindow.Toolbar.Buttons[tbn_filter].Down) then
 MainWindow.Toolbar.Buttons[tbn_filter].Down:= false;
end;

procedure CreateFTTimer;
begin
  if FT<>nil then exit;
  FT:=ttimer.create(nil);
  FT.Interval:=100;
  FT.Ontimer:=@IndicateTbnFilter;
end;

procedure DestroyFTTimer;
begin
  if FT<>nil then
  begin
    FT.Free;
    FT:=nil;
  end;
end;

////////////////////////////////////////////////////////////////////////////////

procedure MainWindowOnDataBaseClose(Sender: TObject);
begin
  DestroyFTTimer;
  if Old_MainWindowOnDataBaseClose<>nil then
  Old_MainWindowOnDataBaseClose(Sender);
end;

procedure ExToolbar(
     ImgListSize:string;
     IconMargins:integer;
     NavBnsDisabled:integer;
     ToolbarMoveDown:integer;
     ToolbarColor:string;
     ToolbarBorder:integer;
     CustomIconFolder:string;
     FilterBnIndicator:integer
);
var SS:TStringStream;
    IL:TImageList;
    i,ii,j,IconsSize:integer;
    IconFileExtentions:array of String;
    IconFile:string;
begin
  if trim(ImgListSize)='' then
  IconsSize := 24 else
  IconsSize := StrToInt(ImgListSize);
  TB_MoveDown := (ToolbarMoveDown=1);
  IL:=TImageList.Create(MainWindow);
  IL.Height := IconsSize;
  IL.Width := IconsSize;
  MainWindow.Toolbar.ButtonHeight:=IL.Height + IconMargins * 2;
  MainWindow.Toolbar.ButtonWidth:=IL.Width + IconMargins * 2;
  NavBnsVisible := (NavBnsDisabled<>1)
    case ToolbarColor of
      'Белый': MainWindow.Toolbar.Color := clWhite;
      'Серебряный': MainWindow.Toolbar.Color := clSilver;
      'Желтый': MainWindow.Toolbar.Color := clYellow;
      'Кремовый': MainWindow.Toolbar.Color := clCream;
    end;
if ToolbarBorder=1 then
MainWindow.Toolbar.EdgeBorders := [ebLeft,ebTop,ebRight,ebBottom]
else MainWindow.Toolbar.EdgeBorders := [];
if DirectoryExists(CustomIconFolder) then
begin
    SetArrayLength(IconFileExtentions,7);
    IconFileExtentions := ['.png','.ico','.bmp','.jpg','.gif','.tif','.tiff'];
for i:=0 to 18 do
    begin
        for j:=0 to GetArrayLength(IconFileExtentions)-1 do
        begin
          IconFile := CustomIconFolder+'\'+IntToStr(i)+IconFileExtentions[j];
          if FileExists(IconFile) then
            begin
              ii:=IL.AddFromFile(IconFile);
              MainWindow.Toolbar.Buttons[TBNByImageIndex(i)].ImageIndex := ii;
            end;
        end;
    end;
end
  else
for i:=0 to 18 do
begin
  SS:=TStringStream.Create(decodebase64(GetStdIco(i),false));
  IL.AddFromStream(SS);
  SS.Free;
end;
MainWindow.Toolbar.Buttons[TBN_MOVEFIRST].ImageIndex := 5 // хз как убрать этот костыль галимый...
MainWindow.Toolbar.Images := IL;
OldMainWindow_pages_OnChange:=MainWindow.pages.OnChange;
MainWindow.pages.OnChange:=@MainWindow_pages_OnChange;

if FilterBnIndicator=1 then
CreateFTTimer;
Old_MainWindowOnDataBaseClose:=MainWindow.OnDataBaseClose;
MainWindow.OnDataBaseClose:=@MainWindowOnDataBaseClose;

end;





